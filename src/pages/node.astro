---
import CardApis from "../components/CardApis.astro";
import bannerNode from "../assets/node.jpg";
import Layout from "../layouts/Layout.astro";


---

<Layout>
  <div class="container mx-auto p-8">
    <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
      <CardApis
        href="/node/movies"
        imageSrc={bannerNode}
        imageAlt="Api de movies con Node.js"
        title="API de películas con Node.js"
        description="Explora la lista de películas con Node.js."
      />

    </div>
  </div>
</Layout>
